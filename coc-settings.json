{
  "css.validate": false,
  "less.validate": false,
  "scss.validate": false,
  "wxss.validate": false,
  "stylelintplus.filetypes": [
    "scss"
  ],
  "stylelintplus.autoFixOnSave": true,
  "cSpell.enabled": false,
  "cSpell.enabledLanguageIds": [
    "markdown"
  ],
  "cSpell.ignoreRegExpList": [
    "`(.*?)`"
  ],
  "markdownlint.config": {
    "code_blocks": false,
    "enableCompoundWords": true,
    "line_length": false,
    "spelling": false
  },
  "coc.preferences.extensionUpdateCheck": "daily",
  "coc.preferences.currentFunctionSymbolAutoUpdate": true,
  "coc.preferences.silentAutoupdate": false,
  "cssmodules.camelCase": true,
  "cssmodules.hintMessage": "cssmodule",
  "prettier.requireConfig": true,
  "prettier.onlyUseLocalVersion": true,
  "eslint.enable": true,
  "eslint.autoFixOnSave": true,
  "eslint.filetypes": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "coc.preferences.formatOnSaveFiletypes": [
    "json",
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  // select the first completed item
  "suggest.noselect": false,
  "suggest.enablePreselect": false,
  "suggest.enablePreview": true,
  // Ignore patterns of matcher module
  "coc.source.file.ignorePatterns": [],
  "javascript.suggestionActions.enabled": false,
  "javascript.preferences.quoteStyle": "double",
  // visual helpers
  "diagnostic.warningSign": "",
  "diagnostic.errorSign": "",
  "diagnostic.hintSign": "",
  "diagnostic.infoSign": "",
  // debug
  //"diagnostic.enable": true,
  //"tsserver.trace.server": "verbose",
  // show error when cursor on line without needing to be on the exact word
  "diagnostic.checkCurrentLine": true,
  "tsserver.npm": "/usr/local/bin/npm",
  // ADD JIRA BACK IN
  "signature.target": "float",
  "signature.preferShownAbove": false,
  "coc.preferences.hoverTarget": "float",
  "diagnostic.virtualText": true,
  "diagnostic.virtualTextCurrentLineOnly": true,
  "suggest.floatEnable": true,
  "diagnostic.messageTarget": "echo",
  "git.addGBlameToVirtualText": false,
  "codeLens.enable": false,
  "suggest.snippetIndicator": "+",
  "codeLens.separator": "|->",
  "suggest.removeDuplicateItems": true,
  "suggest.completionItemKindLabels": {
    "keyword": "\uf1de",
    "variable": "\ue79b",
    "value": "\uf89f",
    "operator": "\u03a8",
    "function": "\u0192",
    "reference": "\ufa46",
    "constant": "\uf8fe",
    "method": "\uf09a",
    "struct": "\ufb44",
    "class": "\uf0e8",
    "interface": "\uf417",
    "text": "\ue612",
    "enum": "\uf435",
    "enumMember": "\uf02b",
    "module": "\uf40d",
    "color": "\ue22b",
    "property": "\ue624",
    "field": "\uf9be",
    "unit": "\uf475",
    "event": "\ufacd",
    "file": "\uf723",
    "folder": "\uf114",
    "snippet": "\ue60b",
    "typeParameter": "\uf728",
    "default": "\uf29c"
  }
}
